{
    "compilerOptions": {
        "baseUrl": "./src",
        "jsx": "react",
        "target": "esnext",
        "module": "esnext",
        "sourceMap": true,
        "removeComments": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "skipDefaultLibCheck": true,
        "allowJs": false,
        "experimentalDecorators": true,
        "checkJs": false,
        "strictNullChecks": true
    },
    "exclude": [
        "./node_modules/**/*"
    ]
}